<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sooying.pay.app.api.dao.collect.MyDaoTest">
	<select id="selectMerchantInfoByMerchantId" parameterType="java.lang.Long"
		resultType="java.lang.Integer">
		select count(*) from dp_business_active_count_info where merchant_id =
		#{merchantId};
	</select>
</mapper>